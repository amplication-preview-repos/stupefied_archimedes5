datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model BlogAnalytics {
  blogPost  String?
  createdAt DateTime @default(now())
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  likes     Int?
  shares    Int?
  updatedAt DateTime @updatedAt
  views     Int?
}
